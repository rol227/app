<h1>
	<%= @organization.name %>
</h1>

<%=link_to 'Schedule', root_path %> | 
<%=link_to 'FAQ', root_path %> | 
<%=link_to 'Important Docs', root_path %> |
<%=link_to 'Overviews', root_path %> |
<%=link_to 'New Event', new_organization_event_path(@organization) %> |
<%=link_to 'V.I.Ps', organization_vips_path(@organization) %>

</br>

<% @event.each do |date, event| %>
    <table>
        <tr>
            <th>
                <%= date %>
            <th>
        </tr>
        <% event.each do |list| %>
            <tr>
                <td>
                    <% list.vip.each do |pls| %>
                        <%= pls.name %>
                    <% end %>
                </td>
            </tr>
        <% end %>
    </table>
<% end %>
