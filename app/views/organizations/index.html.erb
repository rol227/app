<h1>Home</h1>

<p>
	<% if user_signed_in? %>
		<p>Logged in as: </p>
		<%= current_user.email %>
		<%= link_to 'Sign Out', destroy_user_session_path, :method => :delete %>
	<% else %>
		<%= link_to 'Sign In', new_user_session_path %>
	<% end %>
</p>

<% @organizations.each do |org| %>
	<p>Name: </p>
	<p>
		<%= org.name %>
	<p>
<% end %>

<%= link_to 'New', new_organization_path %>
<%= link_to 'Sign Up', new_user_registration_path %>

